a:
mem1 = foreachdir nw,n,ne,e:
	step n
	pickup c,e
endfor
mem1 = foreachdir nw,n,ne:
	step n
	drop
endfor
step se
mem1 = foreachdir nw,n,s,ne,e,se:
	step s
endfor
jump a