mem1 = nearest shredder
mem3 = nearest worker
pickup sw
if myitem != datacube:
	mem4 = nearest worker
	if mem3 == mem4:
		mem2 = nearest datacube
		pickup mem2
		giveto mem1
	endif
else:
	giveto mem1
endif
a:
mem2 = nearest datacube
pickup mem2
giveto mem1
mem2 = nearest datacube
pickup mem2
giveto mem1
mem2 = nearest datacube
pickup mem2
giveto mem1
mem2 = nearest datacube
pickup mem2
giveto mem1
mem2 = nearest datacube
pickup mem2
giveto mem1
mem2 = nearest datacube
pickup mem2
giveto mem1
if mem3 != wall:
	mem3 = nearest wall
	jump a
endif