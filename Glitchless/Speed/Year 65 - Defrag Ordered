-- 7 Billion Humans (2214M) --
-- 65: Defrag Ordered --

jump a
mem1 = foreachdir w:
	mem2 = foreachdir e:
		a:
		if mem1 == nothing:
			if mem2 == nothing:
				comment 0
				step n
				pickup n
				step ne
				step ne
				step ne
				step n
				drop
				step nw
				pickup ne
				step n
				drop
				pickup s
				step e
				step e
				step e
				drop
				step sw
				step sw
				step s
				pickup se
				step nw
				drop
				step sw
				pickup w
				step e
				step e
				step e
				step ne
				step ne
			else:
				comment 1
				step n
				pickup n
				step n
				step ne
				step ne
				step n
				drop
				step ne
				pickup ne
				step nw
				step w
				drop
				step sw
				pickup s
				drop
				step w
				pickup s
				drop
				pickup w
				step e
				step e
				step ne
				drop
				step s
				step se
				step s
				step s
				pickup sw
				step n
				step n
			endif
		else:
			if mem2 != nothing:
				comment 2
				step n
				step n
				step n
				step n
				step n
				pickup e
				step n
				drop
				step e
				pickup se
				drop
				step e
				pickup se
				drop
				step se
				step s
				step s
				pickup sw
				step n
				step n
				drop
				pickup sw
				step w
				step w
				step w
				step w
				step w
				step w
			else:
				comment 3
				step n
				step n
				step n
				step n
				step n
				step n
				pickup w
				step ne
				step e
				drop
				step w
				pickup s
				step w
				step w
				step w
				step sw
				step w
				step w
				drop
				pickup s
				step e
				drop
				step s
				step s
				pickup se
				step ne
				drop
				pickup se
				step w
				step w
			endif
		endif
	endfor
endfor
drop


