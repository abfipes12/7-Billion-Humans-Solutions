-- 7 Billion Humans (2235) --
-- 57: Neighborly Sweeper by @abfipes12 and @aqueous68 --

a:
step nw,sw,s,ne,e
mem1 = set 0
mem2 = foreachdir nw,w,sw,n,s,ne,e,se:
	if nw == 4 and
	 e == 4 and
	 sw == 5 and
	 c != 4 or
	 c > 8 or
	 c == 0 or
	 myitem == datacube and
	 mem2 >= 0:
		pickup c
		mem1 = calc mem1 + 1
		write mem1
	endif
endfor
drop
jump a