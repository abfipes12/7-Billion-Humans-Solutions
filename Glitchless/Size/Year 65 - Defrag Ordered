mem1 = foreachdir nw,w,n,s,ne,e,se:
	step n
endfor
a:
if w != datacube and
 c == datacube and
 myitem != datacube or
 mem2 != datacube:
	pickup c
	step mem2
	mem2 = set myitem
endif
drop
if e == hole:
	step sw
	mem1 = foreachdir nw,w,n,ne,e,se:
		step w
	endfor
else:
	step e
endif
jump a