mem4 = nearest shredder
a:
step n
mem1 = set n
if mem1 == wall:
	pickup mem2
	giveto mem4
endif
if mem2 != datacube and
 mem1 == datacube or
 mem1 < mem2:
	mem2 = set mem1
endif
jump a