-- 7 Billion Humans (2235) --
-- 61: Lazy Pathways by @soerface --

a:
step nw,w,sw,ne,se
if c == datacube and
 c <= 9:
	mem1 = calc c + 1
	mem2 = foreachdir nw,w,sw,n,s,ne,e,se:
		if mem2 == datacube and
		 mem2 > mem1:
			mem3 = set c
			pickup mem2
			write mem1
			drop
			step mem3
		endif
	endfor
endif
jump a