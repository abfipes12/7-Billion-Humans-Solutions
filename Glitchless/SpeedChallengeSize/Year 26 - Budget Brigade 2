-- 7 Billion Humans (2235) --
-- 26: Budget Brigade 2 by @abfipes12 --

if s == worker or
 s == printer:
	a:
	takefrom s
	jump a
endif
b:
if w >= 50 and
 myitem != datacube:
	takefrom w
endif
if e < 50 and
 myitem != datacube:
	takefrom e
endif
if s == shredder and
 myitem == datacube:
	giveto s
endif
jump b