-- 7 Billion Humans (2235) --
-- 38: Seek and Destroy 3 by dmr [https://steamcommunity.com/profiles/76561197960413748] --

mem3 = set s
a:
step n
mem1 = set n
if mem3 == shredder or
 mem1 != nothing and
 mem1 != wall:
	if mem1 < mem2 or
	 mem2 != datacube:
		mem2 = set mem1
	endif
	jump a
endif
if mem2 == datacube:
	pickup mem2
endif
step mem3
if mem2 == datacube:
	drop
endif
b:
mem1 = set se
if mem1 == hole:
	pickup mem2
	giveto mem3
endif
if mem1 < mem2 or
 mem2 != datacube:
	mem2 = set mem1
endif
step e
jump b